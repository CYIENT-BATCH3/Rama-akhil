#include <stdio.h>

void remainder(int *c,int *d)
{
    int e;
    e=*c % *d;
    printf("%d\n",e);
}
void quotient(int *c,int *d)
{
    int f;
    f=*c / *d;
    printf("%d\n",f);
}
void main()
{
    int a,b;
    printf("enter the values");
    scanf("%d %d",&a,&b);
    remainder(&a,&b);
    quotient(&a,&b);
}
