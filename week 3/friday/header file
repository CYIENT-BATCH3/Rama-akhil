#include<stdio.h>
#include"header11.h"
#include"subtract.h"
int main(){
	int var_sum =0,x=0,y=0;
	printf("enter the values any two :\n");
	scanf("%d %d",&x,&y);
	var_sum =sum(x,y);
	int var_sub = sub(x,y);

	printf("\n the sum value is :%d ",var_sum);
	printf("\n the sub value is : %d",var_sub);

}
