#include <stdio.h>
#include <string.h>

int main()
{
  char s1[20];
  char s2[30];
  int i;
  printf("enter the string s1");
  scanf("%s",s1);
  printf("enter the string s2");
  scanf("%s",s2);
  int len1=strlen(s1);
  int len2=strlen(s2);
  for(i=0;i<=len2;i++)
  {
      s1[len1+i]=s2[i];
  }
  printf("%s",s1);
  return 0;
}
