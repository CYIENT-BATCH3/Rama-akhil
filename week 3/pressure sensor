#include<stdio.h>

 int main(){
 	int PR[50]={0x00,0x01,0x02,0x03,0x04,0x05,0x66,0x07,0x08,0x09,0x10,0x11,0x12,0x13,0x14,0x15,0x16,0x17,0x18,0x19,0x20,0x21,0x22,0x23,0x24,0x25,0x26,0x27,0x28,0x29,0x30,0x31,0x32,0x33,0x34,0x35,0x36,0x37,0x38,0x39,0x40,0x41,0x42,0x43,0x44,0x45,0x46,0x47,0x48,0x49};//sensor right samples 20
 	int PC[50]={0x00,0x01,0x02,0x03,0x04,0x05,0x66,0x07,0x08,0x09,0x10,0x11,0x12,0x13,0x14,0x15,0x16,0x17,0x18,0x19,0x20,0x21,0x22,0x23,0x24,0x25,0x26,0x27,0x28,0x29,0x30,0x31,0x32,0x33,0x34,0x35,0x36,0x37,0x38,0x39,0x40,0x41,0x42,0x43,0x44,0x45,0x46,0x47,0x48,0x49};//sensor right samples 20
 	int PL[50]={0x00,0x01,0x02,0x03,0x04,0x05,0x66,0x07,0x08,0x09,0x10,0x11,0x12,0x13,0x14,0x15,0x16,0x17,0x18,0x19,0x20,0x21,0x22,0x23,0x24,0x25,0x26,0x27,0x28,0x29,0x30,0x31,0x32,0x33,0x34,0x35,0x36,0x37,0x38,0x39,0x40,0x41,0x42,0x43,0x44,0x45,0x46,0x47,0x48,0x49};//sensor right samples 20
 	int F_P[50];//final pos array
	int i=0,j=0,k=0,d=0;//for loops
//.........THIS LOOP FOR PRINT ENTIRE VALUE OF SENSORS..........//
	printf("index \t\t PR \t\t PC \t\t PL\n");
		for(int k=0;k<50;k++){
			printf("%d \t\t  %d \t\t %d \t\t %d \n",k,PR[k],PC[k],PL[k]);

		}
	//..........THIS LOOP FOR GET THE NON NOISE DATA...................../////
		j=0;i=0;//intialling zero status
	while( j<50){//loop for read 50

	if(PL[j]>0x05&&PL[j]<0xf5){//non noise data will take

    F_P[i]=PL[j];//assign value to ith array of F_P
    	i++;
    	printf("lengh1 is %d\n",i);
	}
	j=j+5;
	if(PR[j]>0x05&&PR[j]<0xf5){//non noise data will take

    F_P[i]=PR[j];//assign value to ith array of F_P
    	i++;
    	printf("lengh2 is %d\n",i);
	}
	j=j+5;
	if(PC[j]>0x05&&PC[j]<0xf5){//non noise data will take

    F_P[i]=PC[j];//assign value to ith array of F_P
    	i++;
    	printf("lengh3 is %d\n",i);

	}
       	j=j+5;//incresing for loop
	}
	//.....................................................

	int cnt=1;//THIS IS FOR GET ACCURATE VALUE TO DEPLOY AIRBAG
 int lenfp = i;//find length for debug
printf("len is %d",lenfp);//printing
	//................THIS LOOP IS FOR IF VAULE IS MORE THAT 0X55 VALUE THEN IT WILL COUNT SAMPLES
	for(int d=0;d<i;d++){
		 printf("\n0x%x",F_P[d]);//printing non noise data

         if(0x55<=F_P[d]){
	      cnt++;
         }
         else cnt=1;

	}
	    //........IF COUNT SAMPLES IS EQUAL OR MORE THAT 2, IT WILL TELL AIRBAG DEPLOYED
	    if(cnt>3){
	      puts("\nAirbag deployed");

	    }

	}
